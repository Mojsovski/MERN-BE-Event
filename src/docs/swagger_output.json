  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    },
    "schemas": {
      "loginRequest": {
        "type": "object",
        "properties": {
          "identifier": {
            "type": "string",
            "example": "email or username"
          },
          "password": {
            "type": "string",
            "example": "password"
          }
        },
        "xml": {
          "name": "loginRequest"
        }
      },
      "RegisterRequest": {
        "type": "object",
        "properties": {
          "fullName": {
            "type": "string",
            "example": "fullname"
          },
          "userName": {
            "type": "string",
            "example": "username"
          },
          "email": {
            "type": "string",
            "example": "email"
          },
          "password": {
            "type": "string",
            "example": "password"
          },
          "confirmPassword": {
            "type": "string",
            "example": "confirm password"
          }
        },
        "xml": {
          "name": "RegisterRequest"
        }
      },
      "ActivationRequest": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "example": "from email"
          }
        },
        "xml": {
          "name": "ActivationRequest"
        }
      },
      "RemoveMediaRequest": {
        "type": "object",
        "properties": {
          "fileUrl": {
            "type": "string",
            "example": ""
          }
        },
        "xml": {
          "name": "RemoveMediaRequest"
        }
      },
      "CreateCategoryRequest": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "category name"
          },
          "description": {
            "type": "string",
            "example": "category description"
          },
          "icon": {
            "type": "string",
            "example": "category icon "
          }
        },
        "xml": {
          "name": "CreateCategoryRequest"
        }
      },
      "CreateEventRequest": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "event name"
          },
          "banner": {
            "type": "string",
            "example": "image url "
          },
          "category": {
            "type": "string",
            "example": "category id"
          },
          "description": {
            "type": "string",
            "example": "event description"
          },
          "startDate": {
            "type": "string",
            "example": "yyyy-mm-dd hh-mm-ss"
          },
          "endDate": {
            "type": "string",
            "example": "yyyy-mm-dd hh-mm-ss"
          },
          "location": {
            "type": "object",
            "properties": {
              "region": {
                "type": "string",
                "example": "3404070003  id from village"
              },
              "coordinates": {
                "type": "array",
                "example": [
                  0,
                  0
                ],
                "items": {
                  "type": "number"
                }
              },
              "address": {
                "type": "string",
                "example": "address"
              }
            }
          },
          "isOnline": {
            "type": "boolean",
            "example": false
          },
          "isFeatured": {
            "type": "boolean",
            "example": true
          },
          "isPublish": {
            "type": "boolean",
            "example": false
          }
        },
        "xml": {
          "name": "CreateEventRequest"
        }
      },
      "CreateBannerRequest": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "example": "banner title"
          },
          "image": {
            "type": "string",
            "example": "banner image url"
          },
          "isShow": {
            "type": "boolean",
            "example": true
          }
        },
        "xml": {
          "name": "CreateBannerRequest"
